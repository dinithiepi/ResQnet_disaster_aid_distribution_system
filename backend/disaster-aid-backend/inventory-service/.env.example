# Inventory Service Environment Variables
# Copy this to .env and fill in with actual values

# Database connection
DATABASE_URL=postgresql://user:password@host:port/database?sslmode=require

# Fallback database connection (if DATABASE_URL not set)
PGUSER=postgres
PGHOST=localhost
PGDATABASE=disasteraiddb
PGPASSWORD=your_password
PGPORT=5432

# Server port (Railway automatically uses PORT=8080)
PORT=8080
